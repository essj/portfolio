{
	"rulesDirectory": "node_modules/tslint-react/rules",
	"rules": {
		"eofline": true,
		"forin": true,
		"indent": [true, "tabs"],
		"no-consecutive-blank-lines": true,
		"no-for-in-array": true,
		"no-var-keyword": true,
		"one-line": [true, "check-catch", "check-finally", "check-else", "check-open-brace"],
		"prefer-const": [true, {"destructuring": "all"}],
		"whitespace": [true, "check-branch", "check-decl", "check-separator", "check-type-operator"],
		"semicolon": [true, "always"]
	}
}
